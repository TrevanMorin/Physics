      "source": [
        "# PHSX 216 & 218 Plotting Code\n",
        "# Updated for Electric Field Lab Analysis\n",
        "\n",
        "# Import necessary libraries\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "#-----------------------------------------------------------------------#\n",
        "#----------UPDATE THIS SECTION!!!----------\n",
        "\n",
        "# Constants\n",
        "E0 = 1  # Reference electric field in V/m\n",
        "r0 = 1  # Reference radius in m\n",
        "\n",
        "# Data for the plot (normalized to E0 and r0)\n",
        "xvariable_ln_r_mid = np.array([-4.382, -4.046, -3.794, -3.594, -3.427, -3.283, -3.158])  # ln(r_mid/r0)\n",
        "yvariable_ln_E = np.array([6.423, 6.010, 5.850, 5.717, 5.492, 5.285, 5.244])  # ln(E/E0)\n",
        "err_yvariable_ln_E = np.array([0.220, 0.189, 0.187, 0.264, 0.361, 0.386, 0.309])  # Error in ln(E/E0)\n",
        "\n",
        "# Re-assign variables as x, y, dy so that the remaining plotting code may remain generic\n",
        "x = xvariable_ln_r_mid  # ln(r_mid / r0) on the x-axis\n",
        "y = yvariable_ln_E  # ln(E / E0) on the y-axis\n",
        "dy = err_yvariable_ln_E  # Error in ln(E / E0)\n",
        "\n",
        "# UPDATE EACH OF THESE 4 STRINGS FOR YOUR PLOT LABELS AS YOU WANT THEM TO APPEAR\n",
        "\n",
        "plot_title = 'Log-Log Plot of Normalized Electric Field vs. Normalized Radius'\n",
        "x_label = 'ln(r_mid / r0)'\n",
        "y_label = 'ln(E / E0)'\n",
        "slope_units = 'unitless'\n",
        "\n",
        "# UPDATE ANNOTATION PLACEMENT SO IT'S OUT OF WAY OF PLOTTED DATA AND BEST FIT LINE\n",
        "# Integer from 1 to 4, top R corner is 1 (counter-clockwise from there)\n",
        "\n",
        "annotation_placement = 3\n",
        "\n",
        "#-----------------------------------------------------------------------#\n",
        "#-----DON'T NEED TO CHANGE ANYTHING BEYOND THIS POINT!!!-----\n",
        "\n",
        "# Find the intercept and slope, b and m, from Python's polynomial fitting function\n",
        "b, m = np.polynomial.polynomial.polyfit(x, y, 1, w=dy)\n",
        "\n",
        "# Write the equation for the best fit line based on the slope and intercept\n",
        "fit = b + m * x\n",
        "\n",
        "# Calculate the error in slope and intercept\n",
        "def Delta(x, dy):\n",
        "    D = (sum(1 / dy**2)) * (sum(x**2 / dy**2)) - (sum(x / dy**2))**2\n",
        "    return D\n",
        "\n",
        "D = Delta(x, dy)\n",
        "\n",
        "dm = np.sqrt(1 / D * sum(1 / dy**2))  # Error in slope\n",
        "db = np.sqrt(1 / D * sum(x**2 / dy**2))  # Error in intercept\n",
        "\n",
        "# Calculate the \"goodness of fit\" using linear least squares fitting\n",
        "# (value close to number of plotted points indicates good fit)\n",
        "def LLSFD2(x, y, dy):\n",
        "    N = sum(((y - b - m * x) / dy)**2)\n",
        "    return N\n",
        "\n",
        "N = LLSFD2(x, y, dy)\n",
        "\n",
        "# Assign annotation positions based on annotation_placement value\n",
        "if annotation_placement == 2 or annotation_placement == 3:\n",
        "    xpos = 0.05\n",
        "elif annotation_placement == 1 or annotation_placement == 4:\n",
        "    xpos = 0.75\n",
        "\n",
        "if annotation_placement == 1 or annotation_placement == 2:\n",
        "    ypos = [0.9, 0.85, 0.8, 0.75]\n",
        "elif annotation_placement == 3 or annotation_placement == 4:\n",
        "    ypos = [0.2, 0.15, 0.1, 0.05]\n",
        "\n",
        "#-----------------------------------------------------------------------#\n",
        "# Don't need to change anything in this section!\n",
        "\n",
        "# Plot data on graph. Plot error bars and place values for slope,\n",
        "# error in slope, and vertical intercept (plus error) on the plot using \"annotate\"\n",
        "\n",
        "plt.figure(figsize=(15, 10))\n",
        "\n",
        "plt.plot(x, fit, color='green', linestyle='--', label=\"Best Fit Line\")\n",
        "plt.scatter(x, y, color='blue', marker='o', label=\"Data Points\")\n",
        "\n",
        "plt.xlabel(x_label)\n",
        "plt.ylabel(y_label)\n",
        "plt.title(plot_title)\n",
        "\n",
        "plt.errorbar(x, y, yerr=dy, fmt=\"none\", capsize=5, label=\"Error Bars\")  # Don't need to plot x error bars\n",
        "\n",
        "plt.annotate(f'Slope ({slope_units}) = {m:.3f}', (xpos, ypos[0]), xycoords='axes fraction')\n",
        "plt.annotate(f'Error in Slope ({slope_units}) = {dm:.3f}', (xpos, ypos[1]), xycoords='axes fraction')\n",
        "plt.annotate(f'Vertical intercept = {b:.3f}', (xpos, ypos[2]), xycoords='axes fraction')\n",
        "plt.annotate(f'Error in vertical intercept = {db:.3f}', (xpos, ypos[3]), xycoords='axes fraction')\n",
        "\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()\n"
      ]
    }
  ]
}